{
  "name": "earth-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "yarn run build && source-map-explorer 'build/static/js/*.js'",
    "test": "react-scripts test --env=jest-environment-jsdom-sixteen",
    "test:CI": "CI=true coverage run test.py --coverage",  
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/__tests__/**"
    ]
  }
}
